{
  "name": "taro3_react",
  "version": "1.0.0",
  "private": true,
  "description": "taro3_react",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "yarn taro build --type weapp",
    "build:swan": "yarn taro build --type swan",
    "build:alipay": "yarn taro build --type alipay",
    "build:tt": "yarn taro build --type tt",
    "build:h5": "yarn taro build --type h5",
    "build:rn": "yarn taro build --type rn",
    "build:qq": "yarn taro build --type qq",
    "build:jd": "yarn taro build --type jd",
    "build:quickapp": "yarn taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:weapp_online": "cross-env NODE_ENV=production npm run build:weapp -- --watch",
    "build:weapp_online": "cross-env MY_ENV=online CLIENT_ENV=weapp taro build --type weapp",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch  --reset-cache",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "postinstall": "patch-package",
    "push-ios": "cross-env NODE_ENV=codepush yarn build:rn --platform ios && appcenter codepush release -a kafudev/flat_ios -t 1.2.0 --update-contents-path ./dist/ios/CodePush -d Staging",
    "push-android": "cross-env NODE_ENV=codepush yarn build:rn --platform android && appcenter codepush release -a kafudev/flat_android -t 1.2.0 --update-contents-path ./dist/android/CodePush -d Staging",
    "push-ios-history": "appcenter codepush deployment history -a kafudev/flat_ios Staging",
    "push-android-history": "appcenter codepush deployment history -a kafudev/flat_android Staging",
    "push-rollback-ios": "appcenter codepush rollback -a kafudev/flat_ios Staging",
    "push-rollback-android": "appcenter codepush rollback -a kafudev/flat_android Staging",
    "pack-ios": "yarn zip-ios && yarn kafu-cli pack build -p ios -m release -f react-native -s shell -z ./dist/pack/ios/tt.zip --upload",
    "pack-android": "yarn zip-android && yarn kafu-cli pack build -p android -m release -f react-native -s shell -z ./dist/pack/andoid/tt.zip --upload",
    "zip-ios": "yarn build:rn --platform ios && yarn kafu-cli pack zip -p ios -m release -f react-native -z ./dist/pack/ios/tt.zip",
    "zip-android": "yarn build:rn --platform android && yarn kafu-cli pack zip -p android -m release -f react-native -z ./dist/pack/andoid/tt.zip"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@antmjs/vantui": "1.2.5",
    "@kafudev/jcore-react-native": "2.2.3",
    "@kafudev/jpush-react-native": "2.10.7",
    "@kafudev/react-native-alipay": "0.1.10",
    "@kafudev/react-native-vconsole": "0.1.11",
    "@react-native-community/cameraroll": "4.1.2",
    "@react-native-community/geolocation": "2.0.2",
    "@react-native-picker/picker": "2.3.0",
    "@tarojs/components": "3.3.16",
    "@tarojs/react": "3.3.16",
    "@tarojs/redux": "2.2.10",
		"@tarojs/redux-h5": "2.2.10",
    "@tarojs/runtime": "3.3.16",
    "@tarojs/taro": "3.3.16",
    "@tarojs/taro-rn": "3.3.16",
    "axios": "0.24.0",
    "beeshell": "2.0.11",
    "convert-string": "0.1.0",
    "expo-camera": "12.1.2",
    "flyio": "0.6.14",
    "jverification-react-native": "2.5.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.66.4",
    "react-native-code-push": "7.0.4",
    "react-native-collapsible": "1.6.0",
    "react-native-fast-image": "8.5.11",
    "react-native-fs": "2.19.0",
    "react-native-linear-gradient": "2.5.6",
    "react-native-map-linking": "1.0.1",
    "react-native-mmkv": "2.1.0",
    "react-native-webview": "11.17.2",
    "react-native-wechat-lib": "1.1.26",
    "@react-native-community/netinfo": "8.1.0",
    "react-redux": "7.2.6",
    "redux": "4.1.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.4.1",
    "teaset": "0.7.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/runtime": "7.17.2",
    "@kafudev/cli": "1.0.45",
    "@tarojs/cli": "3.3.16",
    "@tarojs/mini-runner": "3.3.16",
    "@tarojs/rn-runner": "3.3.16",
    "@tarojs/webpack-runner": "3.3.16",
    "@types/react": "17.0.39",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "babel-plugin-import": "1.13.3",
    "babel-preset-taro": "3.3.16",
    "cross-env": "7.0.3",
    "eslint": "8.9.0",
    "eslint-config-taro": "3.3.16",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "stylelint": "14.5.2",
    "typescript": "4.5.5"
  }
}
